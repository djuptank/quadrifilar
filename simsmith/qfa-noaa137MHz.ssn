                               <text>//QFA 

NECUnits mm;
// build helix along x axis.
// during wire generation.

f_MHz ;
delta_MHz;
helixTurns ;
helixRatio ;

dcl totalAngle = 360 * helixTurns;


dcl lambda_mm = 3E8 / ( (f_MHz + delta_MHz)  * 1E3)  * 1.01;   // Calculate lambda in mm and add 4.6%
dcl L1Calc = lambda_mm * 1.025;                            // Add 2.5% for Large loop
dcl L2Calc =  lambda_mm * 0.975;                           // Remove 2.5% for small loop

H1 = L1Calc / ( 2 * ( Sqrt( (helixTurns * helixRatio * 3.14)^2 + 1) + helixRatio) ) ;
D1 = H1 * helixRatio;
H2 = L2Calc / ( 2 * ( Sqrt( (helixTurns * helixRatio * 3.14)^2 + 1) + helixRatio) ) ;
D2 = H2 * helixRatio;

dcl scale=1;          // rescale entire geometry
dcl shorten =0 ;                // reduce height on both Large (H1) and Small (H2)
dcl angleLarge = 90; // Angle between large and small helix  

SelectOneOf (NECGrounds) env;  // Select environment
NECGround(env,.005,13);

dcl feedArray = {&quot;Standard&quot;, &quot;Anti-Standard&quot;}; // Select feed
SelectOneOf (feedArray) feedType;

dcl helixHandedness = {&quot;Left-Handed&quot;, &quot;Right-Handed&quot;}; // set helix rotation

SelectOneOf (helixHandedness) helixHand;

if (helixHand == &quot;Left-Handed&quot;){
    // $stepAngle = 6;	                     // positive angle for left
     $stepAngle = totalAngle / (30);
     $rotation = 1;
} else if (helixHand == &quot;Right-Handed&quot;) {
     $stepAngle = 6;                     // negative angle for right
     $rotation = -1;
}

$rotSteps = totalAngle / stepAngle;
Dielectric = 2.8;
Thickness = 1;
height = 1.5k;
dcl feedpoint = 15;

dcl d2 = D2/2;		        // Radius of the helix
dcl d1 =  D1/2;
dcl h2=  H2 ;		// Height Large Helix
dcl h1 = H1  - 4;

  for (int i = 0; i &lt; $rotSteps; ++i) {
	$angle = $stepAngle  *  i  ;	// angle to start of wire segment, small
	$nxtAngle = $angle + $stepAngle;		// angle to end of wire segment, small
       $angleLarge = $angle + 90;            // angle to start of wire segment, large
       $nxtAngleLarge = $nxtAngle + 90;      // angle to end of wire segment, large

      // First make small helix
	dcl froma = {d2, 0,  height - h2 *  (i/$rotSteps)}/_{0, 0,$angle*$rotation};		// start of wire helix a
	dcl toa      = {d2, 0,  height - h2 * ((i+1)/$rotSteps)}/_{0, 0, $nxtAngle*$rotation};	// end of wire
	$wQfa  = NECWire(froma, toa)
     				.insulation  ={Thickness, Dielectric};

	dcl fromb = { -d2, 0, height - h2 *  (i/$rotSteps)}/_{0, 0, $angle*$rotation};		// start of wire helix b
	dcl tob     = { -d2, 0,  height - h2 *  ((i+1)/$rotSteps)}/_{0, 0, $nxtAngle*$rotation};	// end of wire
	$wQfa  = NECWire(fromb, tob)
      				.insulation={Thickness, Dielectric};

     // Second make large helix
     dcl fromLargea = {d1, 0,  height -h1  *  (i/$rotSteps)}/_{0, 0, $angleLarge*$rotation};		// start of wire helix a
     dcl toLargea     = {d1, 0,  height -h1  *  ((i+1)/$rotSteps)}/_{0, 0, $nxtAngleLarge*$rotation};	// end of wire
     $wQfa  = NECWire(fromLargea, toLargea)
                 		.insulation={Thickness, Dielectric};

     dcl fromLargeHelixB = {-d1, 0, height -h1   *  (i/$rotSteps)}/_{0, 0, $angleLarge*$rotation};		// start of wire helix a
     dcl toLargeHelixB     = {-d1, 0, height -h1   *  ((i+1)/$rotSteps)}/_{0, 0, $nxtAngleLarge*$rotation};	// end of wire
     $wQfa  = NECWire(fromLargeHelixB, toLargeHelixB)
    				.insulation={Thickness, Dielectric};
  }



// Large Base
$wQfa  = NECWire({d1, 0, height-h1}/_{0, 0, angleLarge}, {-d1, 0, height-h1}/_{0, 0, angleLarge})
			.insulation={Thickness, Dielectric};

// drive Large Helix
$wireLarge = NECWire({d1, 0, height}/_{0, 0, angleLarge}, {feedpoint, 0, height}/_{0, 0, angleLarge});
$wireLarge = NECWire({-feedpoint, 0, height}/_{0, 0, angleLarge}, {-d1, 0, height}/_{0, 0, angleLarge});
			$wireLarge.insulation={Thickness, Dielectric};

// Small Base
$wQfa  = NECWire({d2, 0, height - h2}, {-d2, 0, height - h2});
			$wQfa.insulation={Thickness, Dielectric};

// drive small Helix
$wire = NECWire({d2, 0, height}, {feedpoint, 0, height});
$wire = NECWire({-feedpoint, 0, height}, {-d2, 0, height});
		//$wire.insulation={Thickness, Dielectric};

// Add feed Dependng on selection Standard/Ainti-Standard

if (feedType == &quot;Standard&quot;) {
    $wire = NECWire({feedpoint, 0, height}/_{0, 0, angleLarge}, {feedpoint, 0, height});
	     	  $wire.insulation={Thickness, Dielectric};
    $wire = NECWire({-feedpoint, 0, height}/_{0, 0, angleLarge}, {-feedpoint, 0, height});
		    $wire.insulation={Thickness, Dielectric};
    $wireLarge = NECWire({feedpoint/Sqrt(2), 0, height}/_{0, 0, angleLarge-45}, {-feedpoint/Sqrt(2), 0, height}/_{0, 0, angleLarge-45});  

} else if (feedType == &quot;Anti-Standard&quot;) {
    $wire = NECWire({-feedpoint, 0, height}/_{0, 0, angleLarge}, {feedpoint, 0, height})
	     	  .insulation={Thickness, Dielectric};
    $wire = NECWire({feedpoint, 0, height}/_{0, 0, angleLarge}, {-feedpoint, 0, height})
		    .insulation={Thickness, Dielectric};
    $wireLarge = NECWire({feedpoint/Sqrt(2), 0, height}/_{0, 0, angleLarge+45}, {-feedpoint/Sqrt(2), 0, height}/_{0, 0, angleLarge+45}); 
		    $wireLarge.insulation={Thickness, Dielectric};
}

// Feed signal

dcl largeDrive = NECSource({drv, gnd}, $wireLarge, 10);  

                     </text>
